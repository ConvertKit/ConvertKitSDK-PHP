    "php": ">=5.5",
    "guzzlehttp/guzzle": "^6.3",
    "monolog/monolog": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
